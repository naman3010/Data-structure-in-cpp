#include<iostream>
using namespace std;


int main(){
    int x, n;
    cin >> x >> n;
  
    cout << power(x, n) << endl;
}



int power(int x, int n) {
    
    int a = n;
  
    if( a == 0)
    {
        return 1;
    }
    
    int y = power(x,n - 1);
    int b = x * y;

        return b;
    

}
